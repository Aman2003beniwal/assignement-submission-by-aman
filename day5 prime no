public class primeNo {
    public static void main(String[] args) {
        int i,j, isprime,n;
        for (i=1;i<=100;i++){
            isprime=0;
            for (j=2;j<=i/2;j++){
                if ((i%j==0)){
                    isprime=1;
                    break ;
                }
            }
            if ((isprime==0)){
                System.out.println(i+" ");
            }
        }

    }
}
